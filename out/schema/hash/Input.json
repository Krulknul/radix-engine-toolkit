{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Input",
  "description": "Hashes data using the hashing algorithm used in Scrypto and the Radix Engine",
  "type": "object",
  "required": [
    "payload"
  ],
  "properties": {
    "payload": {
      "description": "The payload to hash",
      "type": "string",
      "pattern": "[0-9a-fA-F]+"
    }
  }
}