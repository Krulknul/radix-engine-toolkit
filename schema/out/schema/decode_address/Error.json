{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Error",
  "oneOf": [
    {
      "description": "An error emitted when a Bech32 operation fails.",
      "type": "object",
      "required": [
        "error",
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "Error"
          ]
        },
        "error": {
          "type": "object",
          "required": [
            "message"
          ],
          "properties": {
            "message": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "description": "An error emitted when an unexpected Bech32 variant is encountered. In this case, it means that we expected Bech32m but encountered Bech32",
      "type": "object",
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "InvalidBech32Variant"
          ]
        }
      }
    },
    {
      "description": "An error emitted when the address does not have a corresponding entity type",
      "type": "object",
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "NoCorrespondingEntityType"
          ]
        }
      }
    }
  ]
}