{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Error",
  "oneOf": [
    {
      "description": "An error emitted if the passed compiled intent is neither an unsigned, signed, or notarized intent.",
      "type": "object",
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "UnrecognizedTransactionFormat"
          ]
        }
      }
    },
    {
      "description": "An error emitted during the post processing of the invocation",
      "type": "object",
      "oneOf": [
        {
          "description": "An error emitted when a ManifestAstValue visitor fails or reports an error.",
          "type": "object",
          "oneOf": [],
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "ValueVisitorError"
              ]
            }
          }
        },
        {
          "description": "An error emitted when an Instruction visitor fails or reports an error.",
          "type": "object",
          "oneOf": [
            {
              "description": "Errors emitted by the account deposits visitor.",
              "type": "object",
              "oneOf": [
                {
                  "type": "object",
                  "required": [
                    "DuplicateBucketDeclaration"
                  ],
                  "properties": {
                    "DuplicateBucketDeclaration": {
                      "$ref": "#/definitions/BucketId"
                    }
                  },
                  "additionalProperties": false
                },
                {
                  "type": "object",
                  "required": [
                    "UseOfUndeclaredBucket"
                  ],
                  "properties": {
                    "UseOfUndeclaredBucket": {
                      "$ref": "#/definitions/BucketId"
                    }
                  },
                  "additionalProperties": false
                },
                {
                  "type": "object",
                  "required": [
                    "NoCorrespondingResourceChangesForInstruction"
                  ],
                  "properties": {
                    "NoCorrespondingResourceChangesForInstruction": {
                      "type": "integer",
                      "format": "uint32",
                      "minimum": 0.0
                    }
                  },
                  "additionalProperties": false
                }
              ],
              "required": [
                "type"
              ],
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "AccountDepositsVisitorError"
                  ]
                }
              }
            }
          ],
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "InstructionVisitorError"
              ]
            }
          }
        }
      ],
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "PostProcessingError"
          ]
        }
      }
    }
  ],
  "definitions": {
    "BucketId": {
      "description": "Represents a BucketId which uses a transient identifier.",
      "examples": [
        {
          "type": "String",
          "value": "Scrypto"
        },
        {
          "type": "U32",
          "value": "1"
        }
      ],
      "allOf": [
        {
          "$ref": "#/definitions/TransientIdentifier"
        }
      ]
    },
    "TransientIdentifier": {
      "description": "Represents a tagged transient identifier typically used as an identifiers for Scrypto buckets and proofs. Could either be a string or an unsigned 32-bit number (which is serialized as a number and not a string)",
      "oneOf": [
        {
          "examples": [
            {
              "type": "String",
              "value": "Scrypto"
            }
          ],
          "type": "object",
          "required": [
            "type",
            "value"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "String"
              ]
            },
            "value": {
              "description": "A string identifier",
              "type": "string"
            }
          }
        },
        {
          "examples": [
            {
              "type": "U32",
              "value": "1"
            }
          ],
          "type": "object",
          "required": [
            "type",
            "value"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "U32"
              ]
            },
            "value": {
              "description": "A 32-bit unsigned integer which is serialized and deserialized as a string.",
              "type": "string",
              "pattern": "[0-9]+"
            }
          }
        }
      ]
    }
  }
}