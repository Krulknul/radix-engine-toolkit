{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Error",
  "oneOf": [
    {
      "description": "An error emitted if the passed compiled intent is neither an unsigned, signed, or notarized intent.",
      "type": "object",
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "UnrecognizedTransactionFormat"
          ]
        }
      }
    },
    {
      "description": "An error emitted during the post processing of the invocation",
      "type": "object",
      "required": [
        "error",
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "PostProcessingError"
          ]
        },
        "error": {
          "$ref": "#/definitions/VisitorError"
        }
      }
    }
  ],
  "definitions": {
    "VisitorError": {
      "description": "Errors emitted by the various visitors used in the Radix Engine Toolkit.",
      "oneOf": [
        {
          "description": "An error emitted when a ManifestAstValue visitor fails or reports an error.",
          "type": "object",
          "oneOf": [],
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "ValueVisitorError"
              ]
            }
          }
        },
        {
          "description": "An error emitted when an Instruction visitor fails or reports an error.",
          "type": "object",
          "oneOf": [
            {
              "description": "Errors emitted by the account deposits visitor.",
              "type": "object",
              "oneOf": [
                {
                  "type": "object",
                  "required": [
                    "DuplicateBucketDeclaration"
                  ],
                  "properties": {
                    "DuplicateBucketDeclaration": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": false
                },
                {
                  "type": "object",
                  "required": [
                    "UseOfUndeclaredBucket"
                  ],
                  "properties": {
                    "UseOfUndeclaredBucket": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": false
                },
                {
                  "type": "object",
                  "required": [
                    "NoCorrespondingResourceChangesForInstruction"
                  ],
                  "properties": {
                    "NoCorrespondingResourceChangesForInstruction": {
                      "type": "integer",
                      "format": "uint32",
                      "minimum": 0.0
                    }
                  },
                  "additionalProperties": false
                }
              ],
              "required": [
                "type"
              ],
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "AccountDepositsVisitorError"
                  ]
                }
              }
            }
          ],
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "InstructionVisitorError"
              ]
            }
          }
        }
      ]
    }
  }
}