{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Error",
  "oneOf": [
    {
      "description": "An error emitted when the length of the passed data is invalid",
      "type": "object",
      "required": [
        "error",
        "type"
      ],
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "InvalidLength"
          ]
        },
        "error": {
          "type": "object",
          "required": [
            "actual",
            "expected"
          ],
          "properties": {
            "expected": {
              "type": "integer",
              "format": "uint",
              "minimum": 0.0
            },
            "actual": {
              "type": "integer",
              "format": "uint",
              "minimum": 0.0
            }
          }
        }
      }
    }
  ]
}